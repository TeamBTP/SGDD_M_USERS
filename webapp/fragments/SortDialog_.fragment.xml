<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleSortDialogConfirm_">
		<sortItems>
			<ViewSettingsItem text="{i18n>COMPANIA_CODIGO}" key="companyCode" selected="true" />
			<ViewSettingsItem text="{i18n>COMPANIA_NOMBRE}" key="companyName" />
            <ViewSettingsItem text="{i18n>DIVISION_NOMBRE}" key="divisionName" />
            <ViewSettingsItem text="{i18n>SUCURSAL_NOMBRE}" key="officeName" />
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>